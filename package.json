{
  "name": "rapdv-cms",
  "version": "1.0.0",
  "description": "RapDv - Rapid Development Framework - CMS App",
  "author": "Konrad Gadzinowski <konrad@digitaljetty.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run start-dev\"",
    "start-dev": "cross-env NODE_ENV=development nodemon --watch ./submodules/rapdv/server --watch ./server --ext ts,tsx --exec \"ts-node\" ./submodules/rapdv/server/Boot.ts",
    "start-prod": "cross-env NODE_ENV=production \"ts-node\" \"./submodules/rapdv/server/Boot.ts\"",
    "format-code": "prettier --config .prettierrc --ignore-path .prettierignore \"{server/**/*,client/**/*}.{ts,tsx,css,less,scss,js,jsx}\" --write",
    "test-server": "mocha -r ts-node/register server/**/*.test.ts"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "*",
    "@babel/core": "*",
    "@types/body-parser": "*",
